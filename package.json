{
  "name": "datacollection",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production nodemon ./src/app.ts",
    "test": "cross-env NODE_ENV=test jest ",
    "dev": "cross-env NODE_ENV=development nodemon ./src/app.ts",
    "build": "cross-env NODE_ENV=production tsc",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npx eslint src/** --fix",
    "migration": "npm run typeorm:cli migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:gen": "npm run typeorm:cli -- migration:generate -n",
    "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "@types/amqplib": "^0.5.17",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.10",
    "@types/crypto-random-string": "^2.0.0",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/moment": "^2.13.0",
    "amqplib": "^0.7.1",
    "bcrypt": "^5.0.1",
    "better-sqlite3": "^7.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-random-string": "^4.0.0",
    "csv": "^5.5.3",
    "csvtojson": "^2.0.10",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "helmet": "^4.4.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "prettier-eslint": "^12.0.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "save": "^2.4.0",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2",
    "ts-jest": "^26.5.5",
    "tsconfig-paths": "^3.9.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.31",
    "typeorm-seeding": "^1.6.1",
    "typeorm-typedi-extensions": "^0.4.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.21",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.30",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
